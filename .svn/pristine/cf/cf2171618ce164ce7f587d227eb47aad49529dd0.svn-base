---
server:
  port: 8899
spring:
  profiles: dev
  application:
    name: kklplus-provider-golden
  # 数据源配置
  #https://github.com/alibaba/druid/tree/master/druid-spring-boot-starter
  datasource:
    driver-class-name: com.mysql.jdbc.Driver
    type: com.zaxxer.hikari.HikariDataSource
    url: jdbc:mysql://119.23.43.232:18355/kklplus?useUnicode=true&characterEncoding=utf-8&allowMultiQueries=true&useSSL=false
    username: kklplus
    password: kklplus.Test
    hikari:
      minimumIdle: 20
      maximumPoolSize: 50
      autoCommit: true
      idleTimeout: 300000
      poolName: kklplus-web
      maxLifetime: 1800000
      connectionTimeout: 600000
      connectionTestQuery: SELECT 1
  rabbitmq:
    host: 119.23.43.232
    port: 37777
    username: by
    password: rabbitmq@BY123
    publisher-confirms: true
    cache:
      connection:
        mode: channel
    listener:
      simple:
        auto-startup: true
        acknowledge-mode: manual
        concurrency: 10
        max-concurrency: 100
        prefetch: 1
    template:
      retry:
        initial-interval: 5000
        max-attempts: 3
        max-interval: 50000
        multiplier: 1
eureka:
  instance:
    prefer-ip-address: true
    metadata-map:
      zone: dev
      function: kklplus-provider-golden
  client:
    serviceUrl:
      defaultZone: http://dev:dev.123@119.23.43.232:8800/eureka/
    healthcheck:
      enabled: true
info:
  app:
    name: ${spring.application.name}
    desc: 高灯结算微服务
  version: '0.0.1'
management:
  security:
    enabled: false
    roles: SUPERUSER
mybatis:
#  type-aliases-package: com.kkl.kklplus.entity
  mapper-locations: classpath*:/mapper/**/*Mapper.xml
  configuration:
    cache-enabled: true
    map-underscore-to-camel-case: true
logging:
  level:
    org.mybatis: debug
    com.kkl.kklplus: debug
pagehelper:
  helper-dialect: mysql
  reasonable: true
  support-methods-arguments: true
  params: count=countSql
golden:
  callbackUrl: http://localhost:8899
  aesIv: ff465fdecc764337
  okhttp:
    connect-timeout: 10
    read-timeout: 10
    write-timeout: 10
    ping-interval: 0
    retry-on-connection-failure: true
  golden-data-config:
    app-key: 063a424500b0421591df9ea29eab3893
    app-secret: 27da720d54464d3394b4a3ea4792fb3a
    sign-type: sha256
    version: 2.0
    requestMainUrl: https://api-js-ets.wetax.com.cn/api/balance/





