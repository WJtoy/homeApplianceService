      zone: dev
      function: kklplus-provider-golden
  client:
    serviceUrl:
      defaultZone: http://dev:dev.123@@119.23.43.232:8800/eureka/
    healthcheck:
      enabled: true
info:
  app:
    name: ${spring.application.name}
    desc: 高灯结算微服务
  version: '0.0.1'
management:
  security:
    enabled: false
    roles: SUPERUSER
mybatis:
  type-aliases-package: com.kkl.kklplus.entity
  mapper-locations: classpath*:/mapper/**/*Mapper.xml
  configuration:
    cache-enabled: true
    map-underscore-to-camel-case: true
logging:
  level:
    org.mybatis: debug
    com.kkl.kklplus: debug
pagehelper:
  helper-dialect: mysql
  reasonable: true
  support-methods-arguments: true
  params: count=countSql


