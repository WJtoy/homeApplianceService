---
server:
  port: 8899
spring:
  profiles: dev
  application:
    name: kklplus-provider-golden
  # 数据源配置
  #https://github.com/alibaba/druid/tree/master/druid-spring-boot-starter
  datasource:
    driver-class-name: com.mysql.jdbc.Driver
    type: com.zaxxer.hikari.HikariDataSource
    url: jdbc:mysql://119.23.43.232:18355/kklplus_b2b_pdd?useUnicode=true&characterEncoding=utf-8&allowMultiQueries=true&useSSL=false
    username: b2bpdd
    password: pdd123.456
    hikari:
      minimumIdle: 20
      maximumPoolSize: 50
      autoCommit: true
      idleTimeout: 300000
      poolName: kklplus-web
      maxLifetime: 1800000
      connectionTimeout: 600000
      connectionTestQuery: SELECT 1
  rabbitmq:
    host: 119.23.43.232
    port: 37777
    username: by
    password: rabbitmq@BY123
    publisher-confirms: true
    cache:
      connection:
        mode: channel
    listener:
