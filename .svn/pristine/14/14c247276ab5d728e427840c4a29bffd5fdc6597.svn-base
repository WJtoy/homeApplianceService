        settlement_id,
        withdraw_no,
        interface_name,
        info_json,
        result_json,
        process_flag,
        process_time,
        process_comment,
        create_by,
        create_date,
        update_by,
        update_date,
        quarter
        )values (

        #{settlementId},
        #{withdrawNo},
        #{interfaceName}
        #{infoJson},
        #{resultJson},
        #{processFlag},
        #{processTime},
        #{processComment},
        #{createBy},
        #{createDate},
        #{updateBy},
        #{updateDate},
        #{quarter}
        )
    </insert>

    <update id="update" parameterType="com.kkl.kklplus.golden.entity.GoldenCallbackLog">
        UPDATE	golden_callback_log
        <set>
            <include refid="set_column" />
        </set>
        WHERE	ID = #{id,jdbcType=BIGINT}
    </update>



    <sql id="set_column">
