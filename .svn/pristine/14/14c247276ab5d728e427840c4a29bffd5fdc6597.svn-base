        id,
        settlement_id,
        withdraw_no,
        interface_name,
        info_json,
        result_json,
        process_flag,
        process_time,
        process_comment,
        create_by,
        create_date,
        update_by,
        update_date,
        quarter
        from golden_callback_log
        <where>
            <if test="id != null ">
                AND id = #{id,jdbcType=BIGINT}
            </if>
            <if test="settlement_id != null ">
                AND settlement_id = #{settlementId}
            </if>
        </where>
