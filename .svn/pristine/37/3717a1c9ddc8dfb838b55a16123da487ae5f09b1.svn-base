package com.kkl.kklplus.golden.commonEnum;

import lombok.Getter;
import lombok.Setter;

public enum ProcessFlagEnum {

    ACCEPTANCE(0,"受理：接收成功，但是还未处理 0"),
    IMPLEMENT(1,"执行：接收成功，正在执行业务数据处理"),
    REFUSE(2,"拒绝：接收成功，但是执行处理生成业务数据失败，业务数据不满足要求"),
    FAIL(3,"失败：接收成功，但是执行处理生成业务数据报错"),
    SUCCESS(4,"成功：接收成功，并且执行处理生成业务数据成功");

    @Setter
    @Getter
    private Integer value;

    @Setter
    @Getter
    private String name;

    private ProcessFlagEnum(int value, String name) {
        this.value = value;
        this.name = name;
    }

}
